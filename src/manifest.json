            "PropertyInspectorPath": "pi/set-advanced-color.html",
            "States": [
                {
                    "Image": "Images/SetAdvancedColor/State0"
                },
                {
                    "Image": "Images/SetAdvancedColor/State1"
                }
            ],
            "UUID": "com.geekyeggo.windowsdisplaysettings.usehdr"
        },
        {
            "Icon": "Images/SetDisplayResolution/Icon",
            "Name": "Resolution",
            "PropertyInspectorPath": "pi/set-display-resolution.html",
            "States": [
                {
                    "Image": "Images/SetDisplayResolution/Icon"
                }
            ],
            "UUID": "com.geekyeggo.windowsdisplaysettings.resolution"
        },
        {
            "Icon": "Images/SetDisplayOrientation/Icon",
