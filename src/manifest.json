            "PropertyInspectorPath": "pi/set-display-orientation.html",
            "States": [
                {
                    "Image": "Images/SetDisplayOrientation/Icon"
                }
            ],
            "UUID": "com.geekyeggo.windowsdisplaysettings.orientation"
        },
        {
            "Icon": "Images/Project/Icon",
            "Name": "Project",
            "PropertyInspectorPath": "pi/project.html",
            "States": [
                {
                    "Image": "Images/Project/Icon"
                }
            ],
            "UUID": "com.geekyeggo.windowsdisplaysettings.project"
        }
    ],
    "Author": "GeekyEggo",
    "Category": "Windows Display Settings",
    "CategoryIcon": "Images/Plugin/CategoryIcon",
